    <!-- http://www.essepuntato.it/2014/10/disciplinare-upper/haValoreMassimo -->

    <owl:DatatypeProperty rdf:about="http://www.essepuntato.it/2014/10/disciplinare-upper/haValoreMassimo">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>ha valore massimo</rdfs:label>
        <rdfs:comment>Permette di associare un valore massimo per una certa caratteristica o materia prima (ad esempio, &quot;umidità&quot; non superiore al &quot;75%&quot;).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2014/10/disciplinare-upper"/>
        <rdfs:subPropertyOf rdf:resource="http://www.essepuntato.it/2014/10/disciplinare-upper/haValore"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2014/10/disciplinare-upper/haValoreMinimo -->

    <owl:DatatypeProperty rdf:about="http://www.essepuntato.it/2014/10/disciplinare-upper/haValoreMinimo">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>ha valore minimo</rdfs:label>
        <rdfs:comment>Permette di associare un valore minimo per una certa caratteristica o materia prima (ad esempio, &quot;acidità totale&quot; avente un minimo di &quot;6,5 g/l&quot;).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2014/10/disciplinare-upper"/>
        <rdfs:subPropertyOf rdf:resource="http://www.essepuntato.it/2014/10/disciplinare-upper/haValore"/>
    </owl:DatatypeProperty>
    


