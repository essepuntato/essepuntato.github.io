<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY earmark "http://www.essepuntato.it/2008/12/earmark#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY collections "http://swan.mindinformatics.org/ontologies/1.2/collections/" >
]>


<rdf:RDF xmlns="http://www.essepuntato.it/2008/12/earmark#"
     xml:base="http://www.essepuntato.it/2008/12/earmark"
     xmlns:earmark="http://www.essepuntato.it/2008/12/earmark#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:collections="http://swan.mindinformatics.org/ontologies/1.2/collections/">
    <owl:Ontology rdf:about="">
        <rdfs:label rdf:datatype="&xsd;string"
            >EARMARK Ontology</rdfs:label>
        <owl:versionInfo rdf:datatype="&xsd;string">1.7</owl:versionInfo>
        <dc:date rdf:datatype="&xsd;date">2010-03-25</dc:date>
        <dc:contributor rdf:datatype="&xsd;string">Angelo Di Iorio</dc:contributor>
        <dc:title rdf:datatype="&xsd;string"
            >EARMARK Ontology</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Extremely Annotational RDF Markup (EARMARK) is a meta-syntax for non-embedded markup that can be used for stand-off annotations of textual content with fully W3C-compliant technologies.

This document represents the EARMARK ontology definition.</rdfs:comment>
        <dc:contributor rdf:datatype="&xsd;string">Fabio Vitali</dc:contributor>
        <dc:creator rdf:datatype="&xsd;string">Silvio Peroni</dc:creator>
        <dc:language rdf:datatype="&xsd;language">en</dc:language>
        <dc:rights rdf:datatype="&xsd;anyURI"
            >http://creativecommons.org/licenses/by/2.5/it/</dc:rights>
        <dc:publisher rdf:datatype="&xsd;anyURI"
            >http://www.essepuntato.it</dc:publisher>
        <owl:imports rdf:resource="http://swan.mindinformatics.org/ontologies/1.2/collections.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;publisher"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;language"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/element -->

    <owl:ObjectProperty rdf:about="&collections;element"/>
    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/item -->

    <owl:ObjectProperty rdf:about="&collections;item"/>
    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/itemContent -->

    <owl:ObjectProperty rdf:about="&collections;itemContent"/>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#begins -->

    <owl:ObjectProperty rdf:about="#begins">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Begins</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines the starting location of a range.</rdfs:comment>
        <rdfs:range rdf:resource="#Location"/>
        <rdfs:domain rdf:resource="#Range"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#ends -->

    <owl:ObjectProperty rdf:about="#ends">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Ends</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines the ending location of a range.</rdfs:comment>
        <rdfs:range rdf:resource="#Location"/>
        <rdfs:domain rdf:resource="#Range"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#markupContent -->

    <owl:ObjectProperty rdf:about="#markupContent">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&collections;itemContent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MarkupItem"/>
                    <rdf:Description rdf:about="#Range"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#refersTo -->

    <owl:ObjectProperty rdf:about="#refersTo">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Refers To</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It indicates the docuverse the location refers to.</rdfs:comment>
        <rdfs:range rdf:resource="#Docuverse"/>
        <rdfs:domain rdf:resource="#Location"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/size -->

    <owl:DatatypeProperty rdf:about="&collections;size"/>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#actualSize -->

    <owl:DatatypeProperty rdf:about="#actualSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Actual size</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The actual number of item belonging to a collection</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&collections;size"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#at -->

    <owl:DatatypeProperty rdf:about="#at">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">At</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines a precise point in the docuverse.</rdfs:comment>
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#hasContent -->

    <owl:DatatypeProperty rdf:about="#hasContent">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Has Content</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines the docuverse content.</rdfs:comment>
        <rdfs:domain rdf:resource="#Docuverse"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#hasGeneralIdentifier -->

    <owl:DatatypeProperty rdf:about="#hasGeneralIdentifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"
            >Has General Identifier</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It specifies the common name, called general identifier, of a markup item.</rdfs:comment>
        <rdfs:domain rdf:resource="#MarkupItem"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#hasNamespace -->

    <owl:DatatypeProperty rdf:about="#hasNamespace">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Has Namespace</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It specifies the namespace of a markup item.</rdfs:comment>
        <rdfs:domain rdf:resource="#MarkupItem"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#hasText -->

    <owl:DatatypeProperty rdf:about="#hasText">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Has Text</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines the text fragments representing the actual content of the docuverse.</rdfs:comment>
        <rdfs:domain rdf:resource="#StringDocuverse"/>
        <rdfs:subPropertyOf rdf:resource="#hasContent"/>
        <owl:propertyDisjointWith rdf:resource="#hasURI"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#hasURI -->

    <owl:DatatypeProperty rdf:about="#hasURI">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Has URI</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines a reference to the text fragments representing the actual content of the document.</rdfs:comment>
        <rdfs:domain rdf:resource="#URIDocuverse"/>
        <rdfs:subPropertyOf rdf:resource="#hasContent"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/Bag -->

    <owl:Class rdf:about="&collections;Bag"/>
    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/Collection -->

    <owl:Class rdf:about="&collections;Collection"/>
    


    <!-- http://swan.mindinformatics.org/ontologies/1.2/collections/Set -->

    <owl:Class rdf:about="&collections;Set"/>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#Attribute -->

    <owl:Class rdf:about="#Attribute">
        <rdfs:label rdf:datatype="&xsd;string">Attribute</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MarkupItem"/>
        <rdfs:comment rdf:datatype="&xsd;string">An attribute.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#CharNumberLocation -->

    <owl:Class rdf:about="#CharNumberLocation">
        <rdfs:label rdf:datatype="&xsd;string"
            >Char Number Location</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Location"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines a location by counting characters.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#Comment -->

    <owl:Class rdf:about="#Comment">
        <rdfs:label rdf:datatype="&xsd;string">Comment</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MarkupItem"/>
        <rdfs:comment rdf:datatype="&xsd;string">A comment.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#Docuverse -->

    <owl:Class rdf:about="#Docuverse">
        <rdfs:label rdf:datatype="&xsd;string">Docuverse</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A collection of text fragments that can be interconnected to each other and transcluded into new documents.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#Element -->

    <owl:Class rdf:about="#Element">
        <rdfs:label rdf:datatype="&xsd;string">Element</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MarkupItem"/>
        <rdfs:comment rdf:datatype="&xsd;string">An element.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#Location -->

    <owl:Class rdf:about="#Location">
        <rdfs:label rdf:datatype="&xsd;string">Location</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It is the expression of a position in a particular docuverse.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#MarkupItem -->

    <owl:Class rdf:about="#MarkupItem">
        <rdfs:label rdf:datatype="&xsd;string">Markup Item</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&collections;Bag"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&collections;item"/>
                                <owl:onClass rdf:resource="&collections;Collection"/>
                                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:qualifiedCardinality>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&collections;Set"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&collections;element"/>
                                <owl:allValuesFrom>
                                    <owl:Class>
                                        <owl:unionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="#MarkupItem"/>
                                            <rdf:Description rdf:about="#Range"/>
                                        </owl:unionOf>
                                    </owl:Class>
                                </owl:allValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It is the superclass defining artifacts to be interpreted as markup (such as elements, attributes and comments). It is also a collection (List, Bag or Set) of other markup items and ranges.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#Range -->

    <owl:Class rdf:about="#Range">
        <rdfs:label rdf:datatype="&xsd;string">Range</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It refers to any text lying between two locations</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#StringDocuverse -->

    <owl:Class rdf:about="#StringDocuverse">
        <rdfs:label rdf:datatype="&xsd;string"
            >String Docuverse</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Docuverse"/>
        <owl:disjointWith rdf:resource="#URIDocuverse"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This class concerns a docuverse with the content specified as common text.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#URIDocuverse -->

    <owl:Class rdf:about="#URIDocuverse">
        <rdfs:label rdf:datatype="&xsd;string">URI Docuverse</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Docuverse"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This class concerns the docuverse located at a particular URL.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#XPathLocation -->

    <owl:Class rdf:about="#XPathLocation">
        <rdfs:label rdf:datatype="&xsd;string">XPath Location</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Location"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines a location as a node of an XML docuverse defined by an XPath.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.essepuntato.it/2008/12/earmark#XPointerLocation -->

    <owl:Class rdf:about="#XPointerLocation">
        <rdfs:label rdf:datatype="&xsd;string"
            >XPointer Location</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Location"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >It defines a precise point in a XML docuverse according to the Xpointer scheme of the Xpointer standard.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#Attribute"/>
            <rdf:Description rdf:about="#Comment"/>
            <rdf:Description rdf:about="#Element"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#Docuverse"/>
            <rdf:Description rdf:about="#Location"/>
            <rdf:Description rdf:about="#MarkupItem"/>
            <rdf:Description rdf:about="#Range"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#CharNumberLocation"/>
            <rdf:Description rdf:about="#XPathLocation"/>
            <rdf:Description rdf:about="#XPointerLocation"/>
        </owl:members>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Rules
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <swrl:Variable rdf:about="#r2"/>
    <swrl:Variable rdf:about="#i"/>
    <swrl:Variable rdf:about="#d"/>
    <swrl:Variable rdf:about="#e"/>
    <swrl:Variable rdf:about="#l2"/>
    <swrl:Variable rdf:about="#m"/>
    <swrl:Variable rdf:about="#r1"/>
    <swrl:Variable rdf:about="#x"/>
    <swrl:Variable rdf:about="#b"/>
    <swrl:Variable rdf:about="#c"/>
    <swrl:Variable rdf:about="#l1"/>
    <swrl:Variable rdf:about="#s"/>
    <swrl:Variable rdf:about="#y"/>
    <swrl:Imp>
        <swrl:body>
            <swrl:AtomList>
                <rdf:rest>
                    <swrl:AtomList>
                        <rdf:rest>
                            <swrl:AtomList>
                                <rdf:rest>
                                    <swrl:AtomList>
                                        <rdf:first>
                                            <swrl:IndividualPropertyAtom>
                                                <swrl:argument2 rdf:resource="#b"/>
                                                <swrl:propertyPredicate rdf:resource="#begins"/>
                                                <swrl:argument1 rdf:resource="#r2"/>
                                            </swrl:IndividualPropertyAtom>
                                        </rdf:first>
                                        <rdf:rest>
                                            <swrl:AtomList>
                                                <rdf:rest>
                                                    <swrl:AtomList>
                                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                                        <rdf:first>
                                                            <swrl:IndividualPropertyAtom>
                                                                <swrl:argument2 rdf:resource="#e"/>
                                                                <swrl:propertyPredicate rdf:resource="#ends"/>
                                                                <swrl:argument1 rdf:resource="#r2"/>
                                                            </swrl:IndividualPropertyAtom>
                                                        </rdf:first>
                                                    </swrl:AtomList>
                                                </rdf:rest>
                                                <rdf:first>
                                                    <swrl:IndividualPropertyAtom>
                                                        <swrl:argument2 rdf:resource="#e"/>
                                                        <swrl:propertyPredicate rdf:resource="#ends"/>
                                                        <swrl:argument1 rdf:resource="#r1"/>
                                                    </swrl:IndividualPropertyAtom>
                                                </rdf:first>
                                            </swrl:AtomList>
                                        </rdf:rest>
                                    </swrl:AtomList>
                                </rdf:rest>
                                <rdf:first>
                                    <swrl:IndividualPropertyAtom>
                                        <swrl:argument2 rdf:resource="#b"/>
                                        <swrl:propertyPredicate rdf:resource="#begins"/>
                                        <swrl:argument1 rdf:resource="#r1"/>
                                    </swrl:IndividualPropertyAtom>
                                </rdf:first>
                            </swrl:AtomList>
                        </rdf:rest>
                        <rdf:first>
                            <swrl:ClassAtom>
                                <swrl:classPredicate rdf:resource="#Range"/>
                                <swrl:argument1 rdf:resource="#r2"/>
                            </swrl:ClassAtom>
                        </rdf:first>
                    </swrl:AtomList>
                </rdf:rest>
                <rdf:first>
                    <swrl:ClassAtom>
                        <swrl:classPredicate rdf:resource="#Range"/>
                        <swrl:argument1 rdf:resource="#r1"/>
                    </swrl:ClassAtom>
                </rdf:first>
            </swrl:AtomList>
        </swrl:body>
        <swrl:head>
            <swrl:AtomList>
                <rdf:rest rdf:resource="&rdf;nil"/>
                <rdf:first>
                    <swrl:SameIndividualAtom>
                        <swrl:argument1 rdf:resource="#r1"/>
                        <swrl:argument2 rdf:resource="#r2"/>
                    </swrl:SameIndividualAtom>
                </rdf:first>
            </swrl:AtomList>
        </swrl:head>
    </swrl:Imp>
    <swrl:Imp>
        <swrl:head>
            <swrl:AtomList>
                <rdf:rest rdf:resource="&rdf;nil"/>
                <rdf:first>
                    <swrl:DatavaluedPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#hasText"/>
                        <swrl:argument1 rdf:resource="#x"/>
                        <swrl:argument2 rdf:resource="#y"/>
                    </swrl:DatavaluedPropertyAtom>
                </rdf:first>
            </swrl:AtomList>
        </swrl:head>
        <swrl:body>
            <swrl:AtomList>
                <rdf:rest>
                    <swrl:AtomList>
                        <rdf:rest rdf:resource="&rdf;nil"/>
                        <rdf:first>
                            <swrl:DatavaluedPropertyAtom>
                                <swrl:propertyPredicate rdf:resource="#hasContent"/>
                                <swrl:argument1 rdf:resource="#x"/>
                                <swrl:argument2 rdf:resource="#y"/>
                            </swrl:DatavaluedPropertyAtom>
                        </rdf:first>
                    </swrl:AtomList>
                </rdf:rest>
                <rdf:first>
                    <swrl:ClassAtom>
                        <swrl:classPredicate rdf:resource="#StringDocuverse"/>
                        <swrl:argument1 rdf:resource="#x"/>
                    </swrl:ClassAtom>
                </rdf:first>
            </swrl:AtomList>
        </swrl:body>
    </swrl:Imp>
    <swrl:Imp>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This rule means that each bag markup item must contains only items referring to ranges or other markup items.</rdfs:comment>
        <swrl:head>
            <swrl:AtomList>
                <rdf:rest rdf:resource="&rdf;nil"/>
                <rdf:first>
                    <swrl:IndividualPropertyAtom>
                        <swrl:argument2 rdf:resource="#c"/>
                        <swrl:argument1 rdf:resource="#i"/>
                        <swrl:propertyPredicate rdf:resource="#markupContent"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
            </swrl:AtomList>
        </swrl:head>
        <swrl:body>
            <swrl:AtomList>
                <rdf:rest>
                    <swrl:AtomList>
