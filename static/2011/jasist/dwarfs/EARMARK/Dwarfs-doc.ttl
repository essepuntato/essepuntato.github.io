@prefix this:    <http://www.essepuntato.it/2011/jasist/Dwarfs#> .

this:doc
      a       earmark:Element ;
      co:firstItem
              [ co:itemContent this:doc_p
              ] ;
      earmark:hasGeneralIdentifier
              "text"^^xs:string .

this:doc_docuverse
      earmark:hasContent "I like fairy tales. My favourite is ‘Snow White and the Seven Dwarfs’."^^xs:string .

this:doc_p
      a       earmark:Element ;
      co:firstItem
              [ co:itemContent this:doc_r
              ] ;
      earmark:hasGeneralIdentifier
              "p"^^xs:string .

<http://www.essepuntato.it/2011/jasist/Dwarfs>
      a       owl:Ontology .

this:doc_r
      earmark:begins "0"^^xs:nonNegativeInteger ;
      earmark:ends "70"^^xs:nonNegativeInteger ;
      earmark:refersTo this:doc_docuverse .
