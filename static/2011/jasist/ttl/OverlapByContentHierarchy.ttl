@prefix earmark:  <http://www.essepuntato.it/2008/12/earmark#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix co:      <http://swan.mindinformatics.org/ontologies/1.2/collections/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xs:      <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix this:    <http://www.essepuntato.it/2011/jasist/overlapByContentHierarchy#> .

<http://www.essepuntato.it/2011/jasist/overlapByContentHierarchy>
      a       owl:Ontology .

this:p1
      a       earmark:Element ;
      co:firstItem
              [ co:itemContent this:r1
              ] ;
      earmark:hasGeneralIdentifier
              "p"^^xs:string .

this:docuverse1
      earmark:hasContent "Elements p overlap by content hierarchy (rule 2)"^^xs:string .

this:body1
      a       earmark:Element ;
      co:firstItem
              [ co:itemContent this:p1 ;
                co:nextItem
                        [ co:itemContent this:p2
                        ]
              ] ;
      earmark:hasGeneralIdentifier
              "body"^^xs:string .

this:r1
      earmark:begins "0"^^xs:nonNegativeInteger ;
      earmark:ends "19"^^xs:nonNegativeInteger ;
      earmark:refersTo this:docuverse1 .

this:r2
      earmark:begins "19"^^xs:nonNegativeInteger ;
      earmark:ends "48"^^xs:nonNegativeInteger ;
      earmark:refersTo this:docuverse1 .

this:p2
      a       earmark:Element ;
      co:firstItem
              [ co:itemContent this:r1 ;
                co:nextItem
                        [ co:itemContent this:r2
                        ]
              ] ;
      earmark:hasGeneralIdentifier
              "p"^^xs:string .
