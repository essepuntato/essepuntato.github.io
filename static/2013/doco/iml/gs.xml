<?xml version="1.0" encoding="UTF-8"?>
<assign>
	<table>
		<to>table</to>
	</table>
	<list>
		<to>ul</to>
		<to>div[@class=footnote]</to>
	</list>
	<tablebox>
		<to>div[@class=table]</to>
	</tablebox>
	<paragraph>
		<to>p</to>
	</paragraph>
	<section>
		<to>div[@class=section]</to>
		<to>div[@class=abstract]</to>
		<to>div[@class=bibliography]</to>
		<to>div[@class=acknowledgements]</to>
		<to>div[@class=noindex]</to>
		<to>div[@class=notnumbered]</to>
	</section>
	<sectiontitle>
		<to>h1</to>
	</sectiontitle>
	<figure>
		<to>img</to>
	</figure>
	<figurebox>
		<to>div[@class=picture]</to>
	</figurebox>
	<bodymatter>
		<to>body</to>
	</bodymatter>
	<frontmatter>
		<to>head</to>
	</frontmatter>
	<title>
		<to>title</to>
	</title>
	<reference>
		<to>a[@class=ref]</to>
		<to>a[@class=footnote]</to>
	</reference>
	<bibliographicreferencelist>
		<to parent="bibliography">ul</to>
	</bibliographicreferencelist>
	<bibliographicreference>
		<to parent="bibliographicreferencelist">li</to>
	</bibliographicreference>
	<bibliography>
		<to>div[@class=bibliography]</to>
	</bibliography>
	<footnote>
		<to parent="div[@class=footnote]">div</to>
	</footnote>
</assign>